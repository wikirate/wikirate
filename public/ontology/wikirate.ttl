#################################################################

[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :DescendantMetric
                :FormulaMetric
                :InverseRelationMetric
                :RatingMetric
                :RelationMetric
                :ResearchedMetric
                :ScoreMetric
              )
] .


